{
  "name": "araceathlete",
  "version": "4.1.5",
  "description": "Search and Visualize Strava cycling data",
  "main": "main.js",
  "engines": {
    "node": "8.9.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hypersprite/araceathlete.git"
  },
  "scripts": {
    "test": "node_modules/.bin/nodemon src/server/index.js test=true",
    "test:client": "jest",
    "test-postinstall": "webpack --progress --optimize-minimize",
    "clean-assets": "rm -f src/server/public/assets/*",
    "dev": "npm run clean-assets && node_modules/.bin/nodemon --ignore 'src/server/public/' --ignore '*.test.js' src/server/index.js $LOGGING",
    "dev:api": "npm run clean-assets && node_modules/.bin/nodemon --ignore 'src/server/public/' --ignore '*.test.js' --ignore 'src/client/' src/server/index.js $LOGGING",
    "dev-api-only": "NODE_ENV=API-ONLY node_modules/.bin/nodemon src/server/index.js",
    "postinstall": "rm -f src/server/public/assets/* && NODE_ENV=production webpack -p --progress --optimize-minimize",
    "start": "NODE_ENV=production node src/server/index.js",
    "deploy-test": "cd ../ && git subtree push --prefix web origin deploy-heroku-test",
    "deploy": "cd ../ && git subtree push --prefix web origin deploy-heroku-prod",
    "version:patch": "npm version patch && npm add ."
  },
  "author": "Chris Dunavan",
  "license": "Apache-2.0",
  "browserslist": [
    "Chrome > 35",
    "Firefox > 30",
    "ie > 11",
    "iOS > 7",
    "Safari > 7",
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
    "@hypersprite/react-geolocation-hoc": "^1.0.7",
    "@hypersprite/react-vignette": "^1.0.1",
    "@turf/bbox": "^5.1.5",
    "@turf/helpers": "^5.1.5",
    "autoprefixer": "^7.2.4",
    "axios": "^0.17.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-date-fns": "^0.1.0",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-recharts": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "cors": "^2.8.4",
    "css-loader": "^0.28.8",
    "date-fns": "^1.29.0",
    "express": "^4.16.2",
    "express-form": "^0.12.6",
    "fast-csv": "^2.4.1",
    "file-loader": "^1.1.6",
    "google-map-react": "^0.27.0",
    "google-polyline": "^1.0.0",
    "img-loader": "^2.0.0",
    "js-file-download": "^0.4.1",
    "jsonfile": "^4.0.0",
    "just-fns": "^1.6.5",
    "jwt-simple": "^0.5.1",
    "lodash": "^4.17.4",
    "marked": "^0.3.9",
    "material-ui": "^0.20.0",
    "moment": "^2.20.1",
    "moment-timezone": "^0.5.14",
    "mongoose": "^4.13.11",
    "mongoose-findorcreate": "^2.0.0",
    "morgan": "^1.9.0",
    "nexmo": "^2.2.0",
    "node-geocoder": "^3.21.1",
    "node-schedule": "^1.2.5",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.1",
    "passport-local": "^1.0.0",
    "passport-strava-oauth2": "^0.1.3",
    "polyline": "^0.2.0",
    "postcss-loader": "^2.0.10",
    "prop-types": "^15.6.0",
    "qs": "^6.5.1",
    "react": "^16.2.0",
    "react-dimensions": "^1.3.1",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^3.1.3",
    "react-icons": "^2.2.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-tap-event-plugin": "^3.0.2",
    "react-transition-group": "^2.2.1",
    "react-visibility-sensor": "^3.11.0",
    "react-widgets": "^4.1.1",
    "recharts": "^0.22.4",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-form": "^7.2.0",
    "redux-form-material-ui": "^4.3.1",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^5.0.0",
    "redux-thunk": "^2.2.0",
    "requestify": "^0.2.5",
    "serialize-javascript": "^1.4.0",
    "source-map-loader": "^0.2.3",
    "strava-v3": "^1.14.0",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "uuid": "^3.1.0",
    "vhost": "^3.0.2",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-chunk-hash": "^0.5.0",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-manifest-plugin": "^1.3.2",
    "xss-filters": "^1.2.7"
  },
  "devDependencies": {
    "babel-jest": "^21.2.0",
    "flow-bin": "^0.33.0",
    "jest": "^21.2.1",
    "nodemon": "^1.14.8"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "globals": {
      "NODE_ENV": "test"
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/client"
    ]
  }
}
