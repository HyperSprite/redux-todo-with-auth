{
  "name": "araceathlete",
  "version": "5.0.17",
  "description": "Search and Visualize Strava cycling data",
  "main": "main.js",
  "engines": {
    "node": "8.11.1",
    "npm": "6.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hypersprite/araceathlete.git"
  },
  "scripts": {
    "test": "node_modules/.bin/nodemon src/server/index.js test=true",
    "test:client": "jest",
    "test-postinstall": "webpack --progress --optimize-minimize",
    "clean-assets": "rm -f src/server/public/assets/*",
    "dev": "npm run clean-assets && node_modules/.bin/nodemon --ignore 'src/server/public/' --ignore '*.test.js' src/server/index.js $LOGGING",
    "dev:api": "npm run clean-assets && node_modules/.bin/nodemon --ignore 'src/server/public/' --ignore '*.test.js' --ignore 'src/client/' src/server/index.js $LOGGING",
    "dev-api-only": "NODE_ENV=API-ONLY node_modules/.bin/nodemon src/server/index.js",
    "postinstall": "rm -f src/server/public/assets/* && NODE_ENV=production webpack --config webpack.prod.js -p --progress --optimize-minimize",
    "start": "NODE_ENV=production node src/server/index.js",
    "deploy-test": "cd ../ && git subtree push --prefix web origin deploy-heroku-test",
    "deploy": "cd ../ && git subtree push --prefix web origin deploy-heroku-prod",
    "gitadd": "npm --no-git-tag-version version patch && git add ."
  },
  "author": "Chris Dunavan",
  "license": "Apache-2.0",
  "browserslist": [
    "Chrome > 35",
    "Firefox > 30",
    "ie > 11",
    "iOS > 7",
    "Safari > 7",
    "> 1%",
    "last 2 versions"
  ],
  "dependencies": {
